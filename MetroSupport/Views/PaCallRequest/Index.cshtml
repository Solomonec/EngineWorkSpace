                            <div class="request-infobox-context-name">Серийный номер</div> @Html.TextBoxFor(x => x.PaRequest.SerialNumber, new { @class = "request-infobox-context-input", ng_model = "device.serialnumber", @ng_init = "device.serialnumber ='" + Model.PaRequest.SerialNumber + "'", @readonly = "readonly" })
                        </div>
                        <div class="request-infobox-context-container">
                            <div class="request-infobox-context-name">Класс</div> @Html.TextBoxFor(x => x.PaRequest.DeviceClass, new { @class = "request-infobox-context-input", ng_model = "device.deviceclass", @ng_init = "device.deviceclass ='" + Model.PaRequest.DeviceClass + "'", @readonly = "readonly" })
                        </div>
                        <div class="request-infobox-context-container">
                            <div class="request-infobox-context-name">Тип</div> @Html.TextBoxFor(x => x.PaRequest.DeviceType, new { @class = "request-infobox-context-input", ng_model = "device.devicetype", @ng_init = "device.devicetype ='" + Model.PaRequest.DeviceType + "'", @readonly = "readonly" })
                        </div>
                        <div class="request-infobox-context-container">
                            <div class="request-infobox-context-name">Модель</div> @Html.TextBoxFor(x => x.PaRequest.DeviceModel, new { @class = "request-infobox-context-input", ng_model = "device.devicemodel", @ng_init = "device.devicemodel ='" + Model.PaRequest.DeviceModel + "'", @readonly = "readonly" })
                        </div>
                        <div class="request-infobox-context-container">
                            <div class="request-infobox-context-name">Ввод в эксп.</div> @Html.TextBoxFor(x => x.PaRequest.DateInWork, "dd.mm.yyyy", new { @class = "request-infobox-context-input", ng_model = "device.dateinwork", @ng_init = "device.dateinwork ='" + Model.PaRequest.DateInWork + "'", @readonly = "readonly" })
                        </div>
                        <div class="request-infobox-context-container">
                        </div>
                        <div class="request-infobox-context-container">
                        </div>

                    </div>
                </div>
                <div class="request-infobox-lg">
