@model MetroSupport.ViewModels.PaRequest_LogViewModel
@using MetroSupport.Commons
@{
    Layout = null;

}

<!doctype html>
<html ng-app="PaRequestApp">
<head>
    <meta charset="utf-8">
    <title>@Model.PaRequest.RequestNumber - @Model.PaRequest.TroubleSubject</title>
    <link rel="shortcut icon" href="/favicon.ico" type="image/x-icon" />
    @Styles.Render("~/Content/PaRequest")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jquerymodal")
    @Scripts.Render("~/bundles/datepicker")
    @Scripts.Render("~/bundles/angular")
    @Scripts.Render("~/bundles/parequest")


</head>
<body>
    <div class="header-panel" ng-controller="ButtonsControlCtrl">
        <ul style="margin: 0; padding: 0; list-style-image: none;">
            @if (Model.PaRequest.Status == 0)
            {
                if (User.IsInRole("PaBoss") || User.IsInRole("CallCenter"))
                {
                    <li class="button-control-panel"><a href="#" class="button-control-mdsize" ng-click="saverequest()">Назначить и уведомить</a></li>
                }
            }
            @if (Model.PaRequest.Status == 5 || Model.PaRequest.Status == 4)
            {
                if (User.IsInRole("PaWork") || User.IsInRole("PaBoss"))
                {
                    if (Model.User.FullName == Model.PaRequest.AssignTo || Model.User.FullName == Model.PaRequest.AssignBoss)
                    {
                        <li class="button-control-panel"><a href="#" class="button-control-smsize" ng-click="saverequest()">Сохранить</a></li>
                    }
                }
                 if (User.IsInRole("CallCenter"))
                 {
                    <li class="button-control-panel"><a href="#" class="button-control-smsize" ng-click="saverequest()">Сохранить</a></li>
                 }

            }
            @if (Model.PaRequest.Status == 5 & Model.PaRequest.IsWorkingOn == 0)
            {
                if (User.IsInRole("PaWork"))
                {
                    if (Model.User.FullName == Model.PaRequest.AssignTo)
                    {
                        <li class="button-control-panel">@Html.ActionLink("Принять в работу", "Accept", "PaCallRequest", new {@requestid = Model.PaRequest.CallRequestId}, new {@class = "button-control-smsize"})</li>
                    }
                }
               
            }
            @if ((Model.PaRequest.Status == 5 || Model.PaRequest.Status == 4) & Model.PaRequest.IsWorkingOn == 1)
            {
                if (User.IsInRole("PaWork"))
                {
                    if (Model.User.FullName == Model.PaRequest.AssignBoss)
                    {
                        <li class="button-control-panel"><a href="#" class="button-control-smsize" ng-click="openholdon()">Продлить заяку</a></li>
                    }
                }

            }
            @if (Model.PaRequest.Status == 9)
            {
                if (User.IsInRole("PaBoss"))
                {
                    if (Model.User.FullName == Model.PaRequest.AssignBoss)
                    {
                        <li class="button-control-panel"><a href="#" class="button-control-mdsize" ng-click="openreturntoassigner()">Вернуть исполнителю</a></li>
                    }
                }
            }
            @if ((Model.PaRequest.Status == 5 || Model.PaRequest.Status == 4) & Model.PaRequest.IsWorkingOn == 1)
            {
                if (User.IsInRole("PaWork"))
                {
                    if (Model.User.FullName == Model.PaRequest.AssignBoss)
                    {
                        <li class="button-control-panel">@Html.ActionLink("Отправить на подтверждение", "SendForAprove", "PaCallRequest", new {@requestid = Model.PaRequest.CallRequestId}, new {@class = "button-control-lgsize"})</li>
                    }
                }
            }
            @if (Model.PaRequest.Status == 9)
            {
                if (User.IsInRole("PaBoss"))
                {
                    if (Model.User.FullName == Model.PaRequest.AssignBoss)
                    {
                        <li class="button-control-panel">@Html.ActionLink("Закрыть заявку", "CloseRequest", "PaCallRequest", new {@requestid = Model.PaRequest.CallRequestId}, new {@class = "button-control-smsize"})</li>
                    }
                }
            }
            @if ((Model.PaRequest.Status == 5 || Model.PaRequest.Status == 4) & Model.PaRequest.IsWorkingOn == 1)
            {
                if (User.IsInRole("PaWork") || User.IsInRole("PaBoss"))
                {
                    if (Model.User.FullName == Model.PaRequest.AssignBoss || Model.User.FullName == Model.PaRequest.AssignTo)
                    {
                        <li class="button-control-panel"><a href="#" class="button-control-smsize" ng-click="opendenyrequest()">Отклонить</a></li>
                    }
                }
            }
        </ul>
        @Html.Partial("_LoginPartial",Model.User)
    </div>
    <div class="request-header-box">

        <span class="request-header-item-smfont request-header-item-smsize request-header-item-name-color">№ заявки: </span> <span class="request-header-item-smfont request-header-item-smsize request-header-item-content-color">@Model.PaRequest.RequestNumber</span>
        <div class="request-header-clear">  </div>
        <span class="request-header-item-lgfont request-header-item-lgsize request-header-item-name-color">Проблема: </span> <span class="request-header-item-lgfont request-header-item-lgsize-def request-header-item-content-color">@Theme.ThemePrimaryDefault(Model.PaRequest.TroubleSubject)</span>
        <div class="request-header-clear">  </div>
        <span class="request-header-item-smfont request-header-item-smsize request-header-item-name-color ">Дата:</span> <span class="request-header-item-smfont request-header-item-smsize request-header-item-content-color"> @Dates.DateNullConversion(Model.PaRequest.Creation) @Dates.TimeNullConversion(Model.PaRequest.Time)</span>
        <div class="request-header-clear">  </div>
        <span class="request-header-item-smfont request-header-item-smsize request-header-item-name-color">Статус:</span> <span class="request-header-item-smfont request-header-item-smsize request-header-item-content-color">@Status.StatusConvertion(Model.PaRequest.Status, Model.PaRequest.IsWorkingOn)</span>
        <div class="request-header-clear">  </div>
    </div>

    <div class="tab-link-zone">
        <ul id="tabs">
            <li><a href="#" name="tab1">Заявка</a></li>
            <li><a href="#" name="tab2">Работа</a></li>
            <li><a href="#" name="tab3">История</a></li>
        </ul>
    </div>
    <div id="content">
        @using (Html.BeginForm("CreateAndSave", "PaCallRequest", FormMethod.Post, new { id = "pacallrequestform" }))
        {
            @Html.AntiForgeryToken()
            @Html.HiddenFor(x => x.PaRequest.CallRequestId)
            <div id="tab1">
                <div class="request-subject-box" ng-controller="TroubleSubjectCtrl">
                    <div class="request-subject-name">Тема заявки:</div> @Html.TextBoxFor(x => x.PaRequest.TroubleSubject, new { @class = "request-subject-input", ng_model = "theme.themename", @ng_init = "theme.themename ='" + Model.PaRequest.TroubleSubject + "'", @readonly = "readonly" }) <input type="button" ng-click="openthemedialog()" class="request-subject-button" value="..." />
                    <div id="modalthemes" style="display: none">
                        <div class="pa-themes-background">
                            <div class="pa-themes-header">
                                <div class="pa-themes-header-text">Выбирите тему</div>
                            </div>
                            <div class="pa-themes-container">
                                <div class="pa-themes-box">

                                </div>

                            </div>
                            <div style="clear: both"> </div>
                            <div class="pa-themes-control">
                                <button type="button" class="pa-themes-button button-ok" ng-click="themeacception()">Принять</button>
                                <button type="button" class="pa-themes-button button-cancel">Отмена</button>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="request-comment-box">
                    <div class="request-comment-name">Комментарий:</div> @Html.TextAreaFor(x => x.PaRequest.Comment, new { @class = "request-comment-input" })
