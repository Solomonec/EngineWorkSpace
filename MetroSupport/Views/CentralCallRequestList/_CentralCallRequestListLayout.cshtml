@using System.Threading
@using Omu.AwesomeMvc
@using MetroSupport.Helpers
<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <title>Все заявки</title>
    <link rel="shortcut icon" href="/favicon.ico" type="image/x-icon" />
    @Styles.Render("~/Content/CentralRequestList")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryui")
    @Styles.Render("~/Content/themes/base/css")

       
</head>
<body>
    <div class="header-panel">@Html.Action("UserProfile", "Account")</div>
    <div class="logo-box">
        <div class="logo"></div>
    </div>
    <div class="main-request-menu-box">
        <div class="department-menu">
            <ul class="department-point">
                <li class="department-point-it">@Html.ActionLink("IT", "ItOpenCallRequests", "CentralCallRequestList")</li>
                <li class="department-point-svyaz">@Html.ActionLink("Связь", "SvyazOpenCallRequests", "CentralCallRequestList")</li>
                <li class="department-point-aspp">@Html.ActionLink("АСПП", "AsppOpenCallRequests", "CentralCallRequestList")</li>
                <li class="department-point-pa">@Html.ActionLink("ПА", "PaOpenCallRequests", "CentralCallRequestList")</li>
            </ul>
        </div>
      @RenderSection("Header")
    </div>
<div class="main-requests-info-box">
    
      @RenderBody()
    
</div>
   
    <script src="@Url.Content("~/Scripts/AweGrid/AwesomeMvc.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/AweGrid/utils.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/AweGrid/awem.js")" type="text/javascript"></script>
    @Scripts.Render("~/bundles/AweFormatGrid")
</body>
</html>
